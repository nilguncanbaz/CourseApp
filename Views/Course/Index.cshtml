@model CourseStudentsViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Index</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">  
</head>
<body>
      <h1>@Model.Course.Name</h1>

    <ul>
        @foreach (var item in Model.Students)
        {
            <li>
                @item.Name
            </li>
        }
    </ul>
    
</body>
</html>